<script type="text/javascript">
function myFunction() {
  var person = prompt("What is capital of Pakistan", "Islamabad");
  if (person != null& person=='Islamabad') {
    alert("Correct Answer")
  }
  else
  	alert("wrong Answer")
}

</script>
